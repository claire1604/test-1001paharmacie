{
  "name": "Socle-studio-integration",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "rimraf ./build && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run server",
    "eslint-report-html": "eslint src/app -f node_modules/eslint-html-reporter/reporter.js -o eslintReport.html",
    "eslint-fix": "eslint src/app --fix",
    "eslint-jenkins": "eslint -f checkstyle src/app > eslint.xml"
  },
